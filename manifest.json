{
	"manifest_version": 2,
	"name" :"Mega-Debrid for chrome",
	"version" :"1.0",
	"author": "D.Ducatel",
	"description": "Mega-Debrid chrome extension",
	"default_locale" : "en",
	"options_page": "options/options.html",
	"browser_action" :
	{
		"default_icon" : "favicon.png"
	},
	"background": {
		"scripts": ["event.js"],
		"persistent": false
	},
	"permissions": [ 
		"storage",
		"<all_urls>"
	]
}